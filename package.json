{
  "name": "node-red-contrib-scenes",
  "version": "0.1.7",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "builddocs": "typedoc --out ./out/docs/ --readme DOCS.md --mode file --includeDeclarations --excludeExternals tsconfig.json",
    "prepublish": "typings install && tsc",
    "tsinstall": "typings install && tsc",
    "test": "mocha --recursive"
  },
  "license": "MIT",
  "description": "Module that provides scenes for Node RED",
  "keywords": [
    "node-red",
    "scene"
  ],
  "node-red": {
    "nodes": {
      "current-scene": "nodes/current-scene.js",
      "scene": "nodes/scene.js",
      "scene-startstop": "nodes/scene-startstop.js"
    }
  },
  "dependencies": {
    "express": "^4.13.4",
    "when": "^3.7.7"
  },
  "devDependencies": {
    "mocha": "^2.4.5",
    "typedoc": "github:sierrasoftworks/typedoc#v1.8.10",
    "typescript": "^1.8.10",
    "typings": "^0.8.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/denwilliams/node-red-contrib-scenes.git"
  },
  "bugs": {
    "url": "https://github.com/denwilliams/node-red-contrib-scenes/issues"
  },
  "homepage": "https://github.com/denwilliams/node-red-contrib-scenes#readme",
  "author": "Den Williams",
  "directories": {
    "doc": "docs",
    "test": "test"
  }
}
